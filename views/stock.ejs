<%- include("partials/header") %>
<h1><%= stockPageName + " (" + stockPageTitle + ")" %></h1>
<table class="table stock-table">
  <tbody>
    <tr>
      <th scope="row">Price</th>
      <td><%= stockPagePrice %></td>
    </tr>
    <tr>
      <th scope="row">Currency</th>
      <td><%= stockPageCurrency %></td>
    </tr>
    <tr>
      <th scope="row">Open price</th>
      <td><%= stockPagePriceOpen %></td>
    </tr>
    <tr>
      <th scope="row">Close yesterday</th>
      <td><%= stockPageCloseYesterday %></td>
    </tr>
    <tr>
      <th scope="row">Day change</th>
      <td><%= stockPageDayChangePercent + "% (" + stockPageDayChange + ")" %></td>
    </tr>
  </tbody>
</table>

<%- include("partials/footer") %>
